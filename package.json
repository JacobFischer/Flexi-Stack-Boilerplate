  "scripts": {
    "start": "npm run start:server",
    "start:client": "http-server ./dist/client",
    "start:server": "node ./dist/server/main.js",
    "start:static": "http-server ./dist/static",
    "build": "npm run build:client && npm run build:server",
    "build:all": "npm run build:client && npm run build:server && npm run build:static",
    "build:clean": "npm run clean && npm run build",
    "build:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.node.json\" NODE_ENV=\"development\" webpack --progress --mode development",
    "build:server": "npm run build:webpack -- --config src/server/server.webpack.config.ts",
    "build:client": "npm run build:webpack -- --config src/client/client.webpack.config.ts",
    "build:static": "node src/static/",
    "watch": "npm run watch:client",
    "watch:client": "cross-env TS_NODE_PROJECT=\"tsconfig.node.json\" webpack-dev-server --hot --config src/client/client.webpack.config.ts --mode development",
    "watch:server": "npm run build:server -- --watch",
    "clean": "shx rm -rf .cache/ dist/",
    "purge": "npm run clean && shx rm -rf node_modules/ && rm -rf package-lock.json",
    "lint": "eslint ./ --ext .js,.ts,tsx --ignore-pattern dist/",
    "prettier:base": "prettier **/*.{js,ts,tsx}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:check": "npm run prettier:base -- --check",
    "test": "jest --env=node --config ./test/jest.config.js --coverage test",
    "test:update": "npm test -- --updateSnapshot",
    "ncu": "ncu -u"
  }
}
