  "devDependencies": {
    "npm-check-updates": "7.0.4"
  },
  "scripts": {
    "start": "npm run start:server",
    "start:client": "http-server ./dist/client",
    "start:server": "node ./dist/server/main.js",
    "start:static": "http-server ./dist/static",
    "dev": "webpack-dev-server --hot --mode development",
    "build": "npm run build:client && npm run build:server",
    "build:all": "npm run build:client && npm run build:server && npm run build:static",
    "build:clean": "npm run clean && npm run build",
    "build:webpack": "webpack --progress --colors",
    "build:server": "npm run build:webpack -- --config src/server/server.webpack.config.ts --mode development",
    "build:client": "npm run build:webpack -- --config src/client/client.webpack.config.ts --mode development",
    "build:static": "ts-node src/static/index.ts",
    "clean": "shx rm -rf .cache/ dist/",
    "purge": "npm run clean && shx rm -rf node_modules/ && rm -rf package-lock.json",
    "lint": "eslint ./ --ext .js,.ts,tsx --ignore-pattern dist/",
    "prettier:base": "prettier **/*.{js,ts,tsx}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:check": "npm run prettier:base -- --check",
    "test": "jest --env=node --config ./test/jest.config.js --colors --coverage test",
    "test:update": "jest --env=node --config ./test/jest.config.js --colors --updateSnapshot",
    "ncu": "ncu -u"
  }
}
